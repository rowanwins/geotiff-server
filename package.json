  "description": "A Cloud Optimised Geotiff tile server based on geotiff.js",
  "main": "index.js",
  "scripts": {
    "build:dev": "rollup -c -w",
    "build": "cross-env NODE_ENV=production rollup -c",
    "debug": "cross-env webpack-dev-server --config debug/webpack.config.js --mode development --open --hot",
    "lambda:update": "sls deploy function -f app",
    "dev": "run-p build:dev serve",
    "getHarness": "node test/getHarnessFiles.js",
    "serve": "nodemon server.js",
    "start": "node server.js",
    "test:serve": "cross-env NODE_ENV=testLocal npm run dev"
